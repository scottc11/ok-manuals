{
  "name": "ok-manuals",
  "homepage": "https://scottc11.github.io/ok-manuals/#",
  "version": "1.0.0",
  "description": "OK200 Eurorack Module Manuals",
  "private": false,
  "scripts": {
    "deploy": "NODE_ENV=production npm run build && gh-pages -d dist",
    "build": "webpack --config webpack.config.js",
    "watch": "webpack --config webpack.config.js --watch",
    "start": "webpack serve --open",
    "server": "nodemon -r dotenv/config --exec ts-node server/index.ts",
    "server:build": "tsc -p server/tsconfig.json",
    "server:prod": "npm run server:build && node server/dist/index.js",
    "dev": "concurrently \"npm run server\" \"npm start\" --names \"SERVER,CLIENT\" --prefix-colors \"blue,green\"",
    "dev:frontend": "npm start",
    "dev:backend": "npm run server"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@svgr/webpack": "^8.0.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/w3c-web-usb": "^1.0.6",
    "autoprefixer": "^10.4.19",
    "css-loader": "^6.7.1",
    "gh-pages": "^6.3.0",
    "html-webpack-plugin": "^5.5.0",
    "postcss": "^8.4.38",
    "postcss-loader": "^8.1.1",
    "sass": "^1.51.0",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0"
  },
  "dependencies": {
    "@stripe/react-stripe-js": "^3.7.0",
    "@stripe/stripe-js": "^7.5.0",
    "concurrently": "^9.2.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^4.21.2",
    "nodemon": "^3.1.10",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-icons": "^4.8.0",
    "react-router-dom": "^5.3.4",
    "react-use": "^17.4.0",
    "stripe": "^18.3.0",
    "tailwindcss": "^3.4.3"
  }
}
